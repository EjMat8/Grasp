{
  "name": "grasp",
  "version": "1.0.0",
  "description": "HTML CSS JS on an online learning website",
  "default": "index.html",
  "scripts": {
    "watch:sass": "node-sass src/sass/main.scss src/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel watch:sass start:parcel",
    "compile:sass": "node-sass src/sass/main.scss src/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" src/css/style.comp.css -o src/css/style.prefix.css",
    "compress:css": "node-sass src/css/style.prefix.css src/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start:parcel": "parcel index.html",
    "build:parcel": "parcel build index.html --dist-dir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EjMat8/Grasp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EjMat8/Grasp/issues"
  },
  "homepage": "https://github.com/EjMat8/Grasp#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.2",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1"
  }
}
